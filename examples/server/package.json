{
  "name": "@provair/example-server",
  "description": "A provair server example",
  "version": "0.4.0",
  "keywords": [],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "author": "Provair Team",
  "license": "Apache-2.0 ",
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean *example-server-*.tgz dist *.tsbuildinfo package",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run build",
    "start": "node .",
    "start:ts": "ts-node src",
    "verify": "npm pack && tar xf *example-server*.tgz && tree package && npm run clean"
  },
  "dependencies": {
    "@loopback/boot": "^2.5.1",
    "@loopback/core": "^2.9.5",
    "@loopback/rest": "^6.2.0",
    "@loopback/rest-explorer": "^2.2.10",
    "@provair/core": "^0.4.0",
    "chalk": "^4.1.0",
    "debug": "^4.1.1",
    "kvs": "^2.0.4",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.2",
    "@loopback/eslint-config": "^9.0.2",
    "@loopback/testlab": "^3.2.4",
    "@types/debug": "^4.1.5",
    "@types/node": "^14.10.1",
    "eslint": "^7.9.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  }
}
