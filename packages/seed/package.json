{
  "name": "@provair/seed",
  "description": "The seed generator",
  "version": "0.3.0",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/provair/provair.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "author": "Provair Team",
  "license": "Apache-2.0",
  "keywords": [
    "random",
    "seed"
  ],
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean provair-random*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf provair-random*.tgz && tree package && npm run clean"
  },
  "dependencies": {
    "bcrypto": "^5.3.0",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.2",
    "@loopback/testlab": "^3.2.4",
    "@types/node": "^14.10.1"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  }
}
